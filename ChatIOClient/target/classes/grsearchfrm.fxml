<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="367.0" prefWidth="431.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FindGroupController">
    <children>
        <TitledPane animated="false" collapsible="false" layoutX="69.0" layoutY="44.0" prefHeight="89.0" prefWidth="294.0" text="Search for groups">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="txtSearchQuery" layoutX="14.0" layoutY="27.0" onKeyPressed="#txtSearchQueryKeyPressed" prefHeight="25.0" prefWidth="170.0" />
                        <Label layoutX="15.0" layoutY="6.0" text="Group name" />
                        <Button fx:id="btnSearch" layoutX="190.0" layoutY="27.0" mnemonicParsing="false" onAction="#btnSearchClick" prefHeight="25.0" prefWidth="91.0" text="Search" />
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
        <TitledPane animated="false" collapsible="false" layoutX="69.0" layoutY="140.0" prefHeight="173.0" prefWidth="294.0" text="Search results">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <ListView fx:id="lstSearchResults" layoutX="12.0" layoutY="12.0" prefHeight="123.0" prefWidth="268.0" />
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
        <Button fx:id="btnJoin" layoutX="237.0" layoutY="319.0" mnemonicParsing="false" onAction="#btnJoinClick" prefHeight="25.0" prefWidth="126.0" text="Join group" />
        <Label layoutX="70.0" layoutY="323.0" text="Status:" />
        <Label fx:id="lblStatus" layoutX="115.0" layoutY="323.0" />
    </children>
</AnchorPane>
